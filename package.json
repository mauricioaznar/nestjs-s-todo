{
  "name": "nestjs-s-todo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "env-cmd -f ./.env nest start",
    "dev": "env-cmd -f ./.env.develop nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "env-cmd -f ./.env node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:run:test": "env-cmd -f ./.env.test migrate-mongo up",
    "pretest": "npm run migration:run:test",
    "test": "env-cmd -f .env.test jest --watch",
    "mongo:dev:migrate": "env-cmd -f ./.env.develop migrate-mongo up",
    "mongo:dev:status": "env-cmd -f ./.env.develop migrate-mongo status",
    "mongo:prod:migrate": "env-cmd -f ./.env migrate-mongo up",
    "mongo:prod:status": "env-cmd -f ./.env migrate-mongo status",
    "postgre:dev:migrate": "env-cmd -f ./.env.develop prisma migrate dev",
    "postgre:dev:create-migration": "env-cmd -f ./.env.develop prisma migrate dev --create-only",
    "postgre:dev:reset": "env-cmd -f ./.env.develop prisma migrate reset",
    "postgre:dev:generate": "env-cmd -f ./.env.develop prisma generate",
    "postgre:prod:migrate": "env-cmd -f ./.env prisma migrate dev",
    "postgre:prod:generate": "env-cmd -f ./.env prisma generate"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^8.0.9",
    "@nestjs/core": "^8.0.9",
    "@nestjs/graphql": "^9.0.5",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mongoose": "^9.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.9",
    "apollo-server-express": "^3.3.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.6.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "env-cmd": "^10.1.0",
    "graphql": "^15.6.0",
    "graphql-relay": "^0.9.0",
    "graphql-subscriptions": "^1.2.1",
    "graphql-tools": "^8.2.0",
    "graphql-upload": "^12.0.0",
    "moment": "^2.29.1",
    "mongodb": "^4.2.1",
    "mongoose": "^6.0.9",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.2",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.9",
    "@prisma/client": "^3.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^3.4.2",
    "@types/express": "^4.17.13",
    "@types/graphql-relay": "^0.7.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.2",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.4",
    "migrate-mongo": "^8.2.3",
    "prettier": "^2.4.1",
    "prisma": "^3.7.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "jest": {
    "globalSetup": "<rootDir>/__tests__/factories/setup-database.ts",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.test\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
